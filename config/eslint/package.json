{
  "name": "@ria-develop/eslint-config",
  "version": "0.0.1",
  "main": "index.js",
  "private": false,
  "peerDependencies": {
    "eslint": ">= 3",
    "prettier": ">= 2",
    "typescript": ">= 4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "confusing-browser-globals": "^1.0.9",
    "eslint": "^7.8.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.1.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "prettier-eslint": "^11.0.0"
  }
}
